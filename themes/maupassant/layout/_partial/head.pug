    script.
      window.onload=function(){
        var tocLinkArray=jQuery("ol.toc li a");
        for(var i=0;i<tocLinkArray.length;i++){
          var a = tocLinkArray[i];
          var id = a.href.substr(a.href.indexOf("#")+1);
          a["data-id"] = id;
          a.href="javascript:void(0)";
          a.addEventListener("click",function(e){
            var link = e.currentTarget;
            var id = link["data-id"];
            window.scrollTo(0,document.getElementById(decodeURI(id)).offsetTop-220);
          })
        }
      }
    if theme.blogtimer == true
      script.
        window.addEventListener('load',function(){
          var $timer=jQuery("#blog-timer");
          var $desc=jQuery("#blog-timer .desc");
          $timer.show();
          setInterval(function(){
            var now = new Date();
            var start = new Date('2018/07/11');
            var secs = Math.abs(now - start)/1000;
            var day = Math.floor(secs/(24*60*60));
            var hour = Math.floor((secs%(24*60*60))/3600);
            var min = Math.floor(((secs%(24*60*60))%3600)/60);
            var sec = Math.floor(((secs%(24*60*60))%3600)%60);
            var str = "在路上："+day+"天"+hour+"小时"+min+"分钟"+sec+"秒";
            $desc.html(str);
          },1000);
        });
